<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Light" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/lime_500</item>
        <item name="colorPrimaryVariant">@color/our_black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/lime_700</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->

    </style>

    <style name="AlertDialogStyle" parent="Theme.Material3.Dark.Dialog.Alert">
        <item name="android:colorAccent">@color/lime_700</item>
        <item name="android:textColor">@color/our_black</item>
        <item name="android:textColorPrimary">@color/our_black</item>
    </style>
</resources>